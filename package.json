    "babel-preset-react-app": "^9.0.1",
    "bfj": "7.0.1",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "css-loader": "3.2.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
